{
    "$schema": "https://www.raycast.com/schemas/extension.json",
    "name": "timestamp-utils",
    "title": "Timestamp utils",
    "description": "Utils to process timestamp",
    "icon": "timestamp.png",
    "author": "tenfyzhong",
    "owner": "tenfy",
    "categories": [
        "Developer Tools"
    ],
    "license": "MIT",
    "commands": [
        {
            "name": "td",
            "title": "td",
            "subtitle": "timestamp to datetime",
            "description": "Convert timestamp to datetime",
            "mode": "view"
        },
        {
            "name": "dt",
            "title": "dt",
            "subtitle": "datetime to timestamp",
            "description": "Convert datetime to timestamp",
            "mode": "view"
        }
    ],
    "dependencies": {
        "@raycast/api": "^1.56.1"
    },
    "devDependencies": {
        "@raycast/eslint-config": "1.0.5",
        "@types/node": "18.8.3",
        "@types/react": "18.0.9",
        "eslint": "^7.32.0",
        "prettier": "^2.5.1",
        "typescript": "^4.4.3"
    },
    "scripts": {
        "build": "ray build -e dist",
        "dev": "ray develop",
        "fix-lint": "ray lint --fix",
        "lint": "ray lint",
        "publish": "npx @raycast/api@latest publish"
    }
}
